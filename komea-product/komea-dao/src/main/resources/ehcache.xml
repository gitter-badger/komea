<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path=".cache" />
	<defaultCache timeToLiveSeconds="300" timeToIdleSeconds="3600"
		maxElementsInMemory="1000" overflowToDisk="false" />


	<cache name="buildGlobalPeriodTimeSeries" overflowToDisk="true"
		maxElementsInMemory="2" />
	<cache name="buildPeriodTimeSeries" overflowToDisk="true"
		maxElementsInMemory="2" />
	<cache name="buildTimeSeries" overflowToDisk="true"
		maxElementsInMemory="2" />
	<cache name="evaluateKpiValue" overflowToDisk="true"
		maxElementsInMemory="1000" />
	<cache name="evaluateKpiValueOnPeriod" overflowToDisk="true"
		maxElementsInMemory="1000" />
	<cache name="evaluateKpiValues" overflowToDisk="true"
		maxElementsInMemory="30" />
	<cache name="evaluateKpiValuesOnPeriod" overflowToDisk="true"
		maxElementsInMemory="30" />
	<cache name="getKpiValuesAverageOnPeriod" overflowToDisk="true"
		maxElementsInMemory="30" />
	<cache name="getLastStoredValueInHistory" overflowToDisk="true"
		maxElementsInMemory="1000" />









</ehcache>
